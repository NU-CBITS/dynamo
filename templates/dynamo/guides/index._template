<div class="panel panel-default">
  <div class="panel-heading" id="edit-event-toggle-bar">
    <h3 id="current-guide-title" class="panel-title">(%= (collection_name || "Guides") %)</h3>
  </div>
  <div id="guides-list-container" class="guide-view list-group" style="overflow-y:scroll;">
    <div id="guides-list">
    (% _.each(elements, function(e) { %)
      <div class="choose_element list-group-item" data-cid="(%= e.id %)">(%= e.html %)</div>
    (% }) %)
    </div>
  </div>
</div>