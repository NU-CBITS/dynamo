<div class="list-group-item" data-cid="(%= item.cid %)">
   <h4 title="Situation">
      <i class="icon-puzzle-piece icon-cbits-think-feel" style="margin-left: 5px;"></i>
      (%= item.situation %)
      <span class="label pull-right date">
         (%= item.start.toString("h:mm tt, MMM d, yyyy") %)
      </span>
   </h4>
   <p title="Immediate Thought">
      <span class="icon-stack">
         <i class="icon-sign-blank icon-stack-base icon-cbits-connect"></i>
         <i class=" icon-quote-left icon-light"></i>
      </span>
      (%= item.thought %)
   </p>
   <p title="Tags">
      <span class="icon-stack">
         <i class="icon-sign-blank icon-stack-base"></i>
         <i class="icon-tag icon-light"></i>
      </span>
      (% _.each(item.tags.split(", "), function(tag) { 
            print(t.em("#"+tag, {class: "tag", })); 
         });
      %)
   </p>
   <p title="Immediate Emotion"> 
      <span class="icon-stack">
         <i class="icon-sign-blank icon-stack-base icon-cbits-mood"></i>
         <i class="icon-heart-empty icon-light"></i>
      </span
      (%= item.emotion %) 
      (% if (item.emotion_intensity) { %) ((%= item.emotion_intensity %)) (% } %)
   </p>
   <p title="Alternative Thought">
      <span class="icon-stack">
         <i class="icon-sign-blank icon-stack-base icon-cbits-connect"></i>
         <i class="icon-cogs icon-light"></i>
      </span>
      (%= item.alternative_thought %)
   </p>
   <p title="Outcome Emotion">
      <span class="icon-stack">
         <i class="icon-sign-blank icon-stack-base icon-cbits-mood"></i>
         <i class="icon-heart icon-light"></i>
      </span>
      (% if (item.outcome_intensity) { %) ((%= item.outcome_intensity %)) (% } %)
   </p>
</div>