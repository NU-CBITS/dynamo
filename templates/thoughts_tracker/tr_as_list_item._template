<div class="item" data-cid="(%= item.cid %)">
   <div class="stream-icon purple">
      <i class="icon-puzzle-piece"></i>
   </div>
   <p class="date">
      (%= item.start.toString("h:mm tt, MMM d, yyyy") %)
   </p>
   <h4 title="Situation">(%= item.situation %)</h4>
   <div class="row-fluid" title="Immediate Thought">
      <h5 style="font-size:120%">
         <i class="icon-quote-left icon-large"></i> (%= item.thought %) 
      </h5>
   </div>
   <div class="row-fluid" title="Tags">
      <p> 
         (% _.each(item.tags, function(tag) { 
               print(t.em("#"+tag, {class: "tag", })); 
            }); 
         %)
      </p>
   </div>
   <div class="row-fluid" title="Immediate Emotion"> 
      <i class="icon-heart-empty icon-large"></i> 
      (%= item.emotion %) 
      (% if (item.emotion_intensity) { %) ((%= item.emotion_intensity %)) (% } %) 
   </div>
   <div class="row-fluid" title="Alternative Thought"> 
      <i class="icon-cogs icon-large"></i> 
      (%= item.alternative_thought %) 
   </div>
   <div class="row-fluid" title="Outcome Emotion"> 
      <i class="icon-heart icon-large"></i> 
      (% if (item.outcome_intensity) { %) ((%= item.outcome_intensity %)) (% } %) 
   </div>
</div>